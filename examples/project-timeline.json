        "progress": 100,
        "team": "Design"
      },
      {
        "id": "dev1",
        "title": "Core Development",
        "description": "Backend infrastructure and core features",
        "startDate": "2023-05-01",
        "endDate": "2023-09-30",
        "status": "in-progress",
        "progress": 75,
        "team": "Engineering"
      },
      {
        "id": "qa1",
        "title": "Alpha Testing",
        "description": "Internal testing and bug fixes",
        "startDate": "2023-08-15",
        "endDate": "2023-10-15",
        "status": "in-progress",
        "progress": 30,
        "team": "QA"
      },
      {
        "id": "beta",
        "title": "Beta Release",
        "description": "Limited external beta with key clients",
